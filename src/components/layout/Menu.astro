---
import Logo from "../../assets/cleaner-queen-logo.svg"
---

<navbar
  class="fixed top-0 w-full h-16 text-white bg-white flex justify-center items-center z-50"
>
  <div
    class="flex flex-row justify-between items-center w-11/12 max-w-[80rem] h-full py-1"
  >
    <Logo class="w-10/12 max-w-[300px]" />
    <button>
      <svg
        xmlns="http://www.w3.org/2000/svg"
        fill="none"
        viewBox="0 0 24 24"
        stroke-width="1.5"
        stroke="currentColor"
        class="w-8 h-8 text-blue-800"
      >
        <path
          stroke-linecap="round"
          stroke-linejoin="round"
          d="M3.75 6.75h16.5M3.75 12h16.5m-16.5 5.25h16.5"></path>
      </svg>
    </button>
  </div>
</navbar>

<div class="w-11/12 mx-auto max-w-[80rem]"></div>
